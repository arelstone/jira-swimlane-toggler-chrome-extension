{
  "name": "Jira swimlane toggler",
  "version": "0.0.1",
  "manifest_version": 2,
  "author": "Carsten Daureh√∏j",
  "description": "An extension to toggle Jira swimlanes by pressing your keyboard",
  "homepage_url": "https://github.com/arelstone/jira-swimlane-toggler-chrome-extension",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Jira swimlane toggler",
    "default_popup": "view/index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.min.js",
        "index.js"
      ]
    }
  ]
}